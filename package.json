{
  "name": "pearlforge-ui",
  "sideEffects": false,
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "npm run build && node build/server.js",
    "test": "jest --coverage",
    "build": "babel src/server.js --out-dir build && babel src/api --out-dir build/api && webpack --config webpack.prod.js",
    "lint": "eslint src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hartalex/pearlforge-ui.git"
  },
  "author": "alex@hartcode.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hartalex/pearlforge-ui/issues"
  },
  "homepage": "https://github.com/hartalex/pearlforge-ui#readme",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.0.0",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "1.0.1",
    "css-loader": "2.1.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "5.5.3",
    "jest": "24.0.0",
    "prettier": "2.8.8",
    "style-loader": "0.23.1",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-middleware": "6.1.1",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "express": "4.16.4",
    "google-auth-library": "^3.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-google-login": "^5.0.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "testMatch": ["**/src/**/?(*.)+(test).js?(x)"],
    "collectCoverage": true,
    "collectCoverageFrom": ["**/src/**/*.js"]
