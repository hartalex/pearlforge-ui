{
  "name": "pearlforge-ui",
  "sideEffects": false,
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "npm run build && node build/server.js",
    "test": "jest --coverage",
    "build": "babel src/server.js --out-dir build && babel src/api --out-dir build/api && webpack --config webpack.prod.js",
    "lint": "eslint src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hartalex/pearlforge-ui.git"
  },
  "author": "alex@hartcode.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hartalex/pearlforge-ui/issues"
  },
  "homepage": "https://github.com/hartalex/pearlforge-ui#readme",
  "devDependencies": {
    "@babel/cli": "7.22.9",
    "@babel/core": "7.22.9",
    "@babel/preset-env": "7.22.9",
    "@babel/preset-react": "7.22.5",
    "babel-jest": "29.6.2",
    "babel-loader": "9.1.3",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.8.1",
    "eslint": "8.45.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.3",
    "jest": "29.6.2",
    "prettier": "2.8.8",
    "style-loader": "3.3.3",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-middleware": "6.1.1",
    "webpack-merge": "5.9.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "express": "4.18.2",
    "google-auth-library": "9.0.0",
    "prop-types": "^15.8.1",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "react-google-login": "5.2.2",
    "react-redux": "8.1.1",
    "redux": "4.2.1"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "testMatch": [
      "**/src/**/?(*.)+(test).js?(x)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.js"
    ]
  }
}
